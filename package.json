{
  "name": "jsx-directive-react-state",
  "version": "0.0.0-development",
  "description": "jsx directive to map redux state to component props",
  "main": "config",
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "update-lockfile": "greenkeeper-lockfile-update && greenkeeper-lockfile-upload"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kalispera/jsx-directive-react-state.git"
  },
  "keywords": [
    "state",
    "react",
    "redux",
    "directive",
    "jsx"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kalispera/jsx-directive-react-state/issues"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!node_modules/**",
      "!coverage/**",
      "!config.js"
    ],
    "setupFiles": [
      "raf/polyfill"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "homepage": "https://github.com/kalispera/jsx-directive-react-state#readme",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-jest": "23.0.0-alpha.0",
    "babel-plugin-syntax-jsx": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "codecov": "3.0.0",
    "condition-circle": "2.0.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "greenkeeper-lockfile": "1.13.3",
    "jest": "22.1.4",
    "raf": "3.4.0",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "react-test-renderer": "16.3.1",
    "semantic-release": "15.1.4",
    "strip-ansi": "4.0.0"
  },
  "peerDependencies": {
    "react": "^15.6.1"
  },
  "dependencies": {
    "firebase": "4.12.1",
    "prop-types": "15.6.1",
    "react-redux": "5.0.7",
    "redux": "3.7.2",
    "redux-devtools-extension": "2.13.2"
  },
  "release": {
    "verifyConditions": "condition-circle"
  }
}
