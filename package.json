{
  "name": "jsx-directive-react-state",
  "version": "0.0.0-development",
  "description": "jsx directive to map redux state to component props",
  "main": "config",
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "semantic-release":
      "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kalispera/jsx-directive-react-state.git"
  },
  "keywords": ["state", "react", "redux", "directive", "jsx"],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kalispera/jsx-directive-react-state/issues"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!node_modules/**",
      "!coverage/**",
      "!config.js"
    ],
    "setupFiles": ["raf/polyfill"],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "homepage": "https://github.com/kalispera/jsx-directive-react-state#readme",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-jest": "21.2.0",
    "babel-plugin-syntax-jsx": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "codecov": "3.0.0",
    "condition-circle": "1.6.0",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "eslint": "4.13.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.5.1",
    "jest": "21.2.1",
    "raf": "3.4.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-test-renderer": "16.2.0",
    "semantic-release": "8.2.0",
    "strip-ansi": "4.0.0"
  },
  "peerDependencies": {
    "react": "^15.6.1"
  },
  "dependencies": {
    "firebase": "4.8.0",
    "prop-types": "15.6.0",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-devtools-extension": "2.13.2"
  },
  "release": {
    "verifyConditions": "condition-circle"
  }
}
